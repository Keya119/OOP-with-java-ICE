
package keya.java;

import java.util.Scanner;
public class KeyaJava {
          
    
    public static void main(String[] args) {
       
        int i,n,sum=0;
        System.out.print("Enter a number : ");
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
       
        
        for(i=1;i<=n;i+=2);
        
        {
            sum+=i;
        }
        System.out.println(" sum of the natural odd numbers:"+sum);
    }
    
}
